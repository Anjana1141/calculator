<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>

	<servlet>
		<servlet-name>Addition</servlet-name>
		<servlet-class>com.calculator.AddNumber</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>Substraction</servlet-name>
		<servlet-class>com.calculator.SubstractNumber</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>Multiplication</servlet-name>
		<servlet-class>com.calculator.MultiplyNumber</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>Division</servlet-name>
		<servlet-class>com.calculator.DivideNumber</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>Modulus</servlet-name>
		<servlet-class>com.calculator.ModulusNumber</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>Addition</servlet-name>
		<url-pattern>/AddNumber</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Substraction</servlet-name>
		<url-pattern>/SubstractNumber</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Multiplication</servlet-name>
		<url-pattern>/multiplication</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Division</servlet-name>
		<url-pattern>/Division</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Modulus</servlet-name>
		<url-pattern>/Modulus</url-pattern>
	</servlet-mapping>

</web-app>
